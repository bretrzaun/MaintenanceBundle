{
    "name": "bretrzaun/maintenance-bundle",
    "description": "Bundle for maintenance switch in Symfony applications",
    "type": "bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Bret R. Zaun",
            "email": "bretrzaun@hotmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "BretRZaun\\MaintenanceBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BretRZaun\\MaintenanceBundle\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.1",
        "symfony/yaml": "^6.0||^7.0",
        "twig/twig": "^3.0"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^7.0",
        "symfony/framework-bundle": "^6.0||^7.0",
        "symfony/twig-bundle": "^6.0||^7.0",
        "symfony/asset": "^6.0||^7.0",
        "symfony/browser-kit": "^6.0||^7.0",
        "symfony/translation": "^6.0||^7.0",
        "symfony/var-dumper": "^6.0||^7.0",
        "phpunit/phpunit": "^10.5",
        "phpstan/phpstan": "^2.0",
        "rector/rector": "^2.0"
    },
    "scripts": {
        "rector": "rector",
        "test": "phpunit --no-coverage"
    }
}
